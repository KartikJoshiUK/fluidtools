{
  "name": "fluidtools",
  "version": "1.0.16",
  "description": "AI-powered API multi tool agent with multi-provider support (OpenAI, Anthropic, Ollama)",
  "homepage": "https://github.com/KartikJoshiUK/fluidtools#readme",
  "bugs": {
    "url": "https://github.com/KartikJoshiUK/fluidtools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KartikJoshiUK/fluidtools.git"
  },
  "license": "ISC",
  "author": "Kartik Joshi",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./client": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./tools": {
      "types": "./dist/types/index.d.ts", 
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js", 
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/demo",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "ollama",
    "langchain",
    "agent",
    "api"
  ],
  "scripts": {
    "build": "tsup --minify",
    "build:dev": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "tsup --minify --sourcemap",
    "build:analyze": "npm run build:prod && echo Build completed. Check dist/ for output files and source maps.",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.0.1",
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^1.0.5",
    "@langchain/google-genai": "^1.0.1",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/ollama": "^1.0.1",
    "@langchain/openai": "^1.1.1",
    "axios": "^1.13.2",
    "dotenv": "^16.6.1",
    "langchain": "^1.0.4",
    "langsmith": "^0.3.79",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "fast-check": "^4.4.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1"
  },
  "bin": {
    "fluidtools": "./dist/cjs/cli.cjs"
  }
}
