{
  "name": "fluidtools",
  "version": "1.0.21",
  "description": "AI-powered API multi tool agent with multi-provider support (OpenAI, Anthropic, Ollama)",
  "homepage": "https://github.com/KartikJoshiUK/fluidtools#readme",
  "bugs": {
    "url": "https://github.com/KartikJoshiUK/fluidtools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KartikJoshiUK/fluidtools.git"
  },
  "license": "ISC",
  "author": "Kartik Joshi",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "!dist/demo",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "ollama",
    "langchain",
    "agent",
    "api"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.0.1",
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^1.0.5",
    "@langchain/google-genai": "^1.0.1",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/ollama": "^1.0.1",
    "@langchain/openai": "^1.1.1",
    "axios": "^1.13.2",
    "dotenv": "^16.6.1",
    "langchain": "^1.0.4",
    "langsmith": "^0.3.79",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1"
  },
  "bin": {
    "fluidtools": "dist/src/converters/cli.js"
  }
}